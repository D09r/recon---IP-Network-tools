{
    "update_url": "https://clients2.google.com/service/update2/crx",
	"manifest_version": 2,
	"name": "recon - IP & Network tools",
	"description": "Recon, a collection of an IP & Network tools that can be used to get information about IP Addresses, Web Pages, and DNS records.",
	"version": "0.0.4",	
	"short_name": "recon",
    "author": "D09r",
    "homepage_url": "https://chrome.google.com/webstore/detail/recon-ip-network-tools/lpfpoenklfncdgminmpdoomkbjaiolod",
	"icons": {
		"16": "img/r3_16.png",
        "32": "img/r3_32.png",
		"64": "img/r3_64.png",
        "128": "img/r3_128.png"
	},
  	"permissions": [
		"tabs",
        "background",
        "*://api.hackertarget.com/*"
  	],
	"background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "options_page": "recon.html",
	"browser_action": {
        "default_icon": "img/r3_16.png",
        "default_title": "recon - IP & Network tools"
    }
}